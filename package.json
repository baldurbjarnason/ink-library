{
  "name": "ink-library",
  "version": "0.1.0",
  "main": "index.js",
  "description": "The ink library frontend",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "sapper dev",
    "build": "tsc && sapper build sapper --legacy && node save-keyfile.js",
    "build:typescript": "tsc",
    "export": "sapper export --legacy",
    "start": "node index.js",
    "lint": "healthier && svelte-check",
    "snapshots": "TAP_SNAPSHOT=1 npm run test && prettier --write --plugin-search-dir=. 'src/**/*.{js,json,css,yml,svelte}'",
    "test": "web-test-runner test/**/*.test.ts --coverage",
    "watch:test": "web-test-runner test/**/*.test.js --node-resolve --coverage --watch",
    "watch:typescript": "tsc --watch",
    "watch": "npm-run-all --parallel watch:*",
    "deploy-function": "cd functions/onfinalize && gcloud functions deploy triggerWrapper --runtime nodejs12 --trigger-resource ink-upload-files --trigger-event google.storage.object.finalize --memory=2048MB --timeout=300s --project fb-ink-dev",
    "deploy-dev-function": "cd functions/onfinalize && gcloud functions deploy triggerWrapper --runtime nodejs12 --trigger-resource ink-frontend-server-dev-uploads --trigger-event google.storage.object.finalize --project ink-frontend-server-dev --memory=2048MB --timeout=300s",
    "deploy-dev": "NODE_ENV=development npm run build && gcloud app deploy app-dev.yaml --project ink-frontend-server-dev",
    "deploy-production": "npm run build && gcloud app deploy app.yaml --project fb-ink-dev",
    "save-config": "node save-config.js",
    "save-keyfile": "node save-keyfile.js"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-easy-import": {},
      "postcss-calc": {},
      "cssnano": {}
    }
  },
  "engines": {
    "node": ">10"
  },
  "healthier": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "ignore": [
      "__sapper__",
      "rollup.config.js"
    ]
  },
  "dependencies": {
    "@github/details-menu-element": "^1.0.9",
    "@google-cloud/firestore": "^4.4.0",
    "@google-cloud/storage": "^5.3.0",
    "@popperjs/core": "^2.6.0",
    "@types/awesomplete": "^1.1.10",
    "@types/compression": "^1.7.0",
    "@types/dompurify": "^2.0.4",
    "@types/node": "^14.14.7",
    "@types/quill": "^2.0.4",
    "a11y-dialog": "^5.3.2",
    "autoprefixer": "^9.8.6",
    "awesomplete": "^1.1.5",
    "bcrypt": "^5.0.0",
    "compressible": "^2.0.18",
    "connect-redis": "^4.0.4",
    "cssnano": "^4.1.10",
    "csurf": "^1.11.0",
    "date-fns": "^2.15.0",
    "debug": "^4.1.1",
    "document-register-element": "^1.14.5",
    "dom-anchor-text-position": "^5.0.0",
    "dom-anchor-text-quote": "^4.0.2",
    "dom-seek": "^5.1.1",
    "dompurify": "^2.2.6",
    "express": "^4.17.3",
    "express-session": "^1.17.1",
    "fg-modal": "^0.1.0",
    "got": "^10.7.0",
    "inert-polyfill": "^0.2.5",
    "ink-engine": "github:RebusFoundation/ink-engine",
    "iso-639-1": "^2.1.3",
    "jsdom": "^16.4.0",
    "jsonwebtoken": "^8.5.1",
    "link-preview-js": "^2.1.8",
    "markdown-it": "^11.0.0",
    "mime": "^2.4.6",
    "mime-types": "^2.1.27",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "postcss": "^7.0.36",
    "postcss-calc": "^7.0.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-reporter": "^6.0.1",
    "qsa-observer": "^1.1.6",
    "quill": "^1.3.7",
    "redis": "^3.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.6.3",
    "sharp": "^0.24.1",
    "short-uuid": "^3.1.1",
    "striptags": "^3.2.0",
    "tslib": "^2.0.3",
    "turndown": "^7.0.0",
    "universal-base64url": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^2.5.32",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@rollup/plugin-url": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/mocha": "^8.2.0",
    "@web/dev-server": "^0.1.8",
    "@web/dev-server-esbuild": "^0.2.11",
    "@web/test-runner": "^0.12.15",
    "@web/test-runner-chrome": "^0.9.4",
    "@web/test-runner-puppeteer": "^0.9.3",
    "compression": "^1.7.4",
    "dotenv": "^8.6.0",
    "faker": "^4.1.0",
    "fetch-mock": "^9.11.0",
    "friendly-words": "^1.1.10",
    "globby": "^11.0.2",
    "healthier": "3.2.0",
    "intersection-observer": "^0.7.0",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^2.1.0",
    "prettier": "1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.41.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "sirv": "^0.4.6",
    "snowpack": "^3.0.10",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.5",
    "svelte-dnd-action": "^0.6.22",
    "svelte-preprocess": "^4.6.9",
    "tap": "^14.10.8",
    "tiny-lr": "^1.1.1",
    "typescript": "^4.0.5"
  }
}
